{
	"name": "obsidian-structural-editing",
	"version": "0.0.4",
	"description": "Structure aware editor commands, including smart grow/shrink of the selection",
	"main": "main.js",
	"scripts": {
		"grammar": "antlr4ts -visitor -o src/grammar *.g4",
		"dev": "node esbuild.config.mjs",
		"test": "jest",
		"lint": "eslint ./src --fix && eslint ./test --fix",
		"lint-more": "eslint ./src --fix && eslint ./test --fix && tsc --noEmit --pretty # how to exclude generated ?",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"clean": "rm  -rf ./out && rm -rf src/grammar"
	},
	"keywords": [
		"parser",
		"markdown",
		"obsidian"
	],
	"author": "mvdkwast",
	"license": "MIT",
	"devDependencies": {
		"@babel/plugin-transform-modules-commonjs": "^7.19.6",
		"@types/jest": "^29.2.2",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"antlr4ts-cli": "0.5.0-alpha.4",
		"babel-jest": "^29.3.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.2.2",
		"obsidian": "latest",
		"prettier": "^2.7.1",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@babel/preset-env": "^7.20.2",
		"@codemirror/language": "^6.3.0",
		"@codemirror/state": "^6.1.2",
		"@codemirror/view": "^6.4.0",
		"antlr4ts": "0.5.0-alpha.4",
		"mdast-util-from-markdown": "^1.2.0",
		"mdast-util-frontmatter": "^1.0.0",
		"mdast-util-gfm": "^2.0.1",
		"mdast-util-gfm-autolink-literal": "^1.0.2",
		"mdast-util-gfm-footnote": "^1.0.1",
		"mdast-util-gfm-strikethrough": "^1.0.2",
		"mdast-util-gfm-table": "^1.0.6",
		"mdast-util-gfm-task-list-item": "^1.0.1",
		"mdast-util-math": "^2.0.1",
		"micromark-extension-frontmatter": "^1.0.0",
		"micromark-extension-gfm": "^2.0.1",
		"micromark-extension-gfm-autolink-literal": "^1.0.3",
		"micromark-extension-gfm-footnote": "^1.0.4",
		"micromark-extension-gfm-strikethrough": "^1.0.4",
		"micromark-extension-gfm-table": "^1.0.5",
		"micromark-extension-gfm-task-list-item": "^1.0.3",
		"micromark-extension-math": "^2.0.2"
	}
}
